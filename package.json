{
  "name": "o2",
  "version": "2.4.1",
  "description": "This is a plugin to make obsidian markdown syntax compatible with other markdown syntax.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest --coverage",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prettier:write": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "prepare": "husky"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "jekyll"
  ],
  "author": "haril song",
  "license": "MIT",
  "devDependencies": {
    "@js-temporal/polyfill": "^0.5.1",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "8.33.0",
    "@typescript-eslint/parser": "8.33.0",
    "builtin-modules": "5.0.0",
    "esbuild": "0.25.3",
    "eslint": "^9.23.0",
    "eslint-plugin-yml": "^1.17.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.0",
    "obsidian": "^1.8.7",
    "prettier": "^3.5.3",
    "process": "^0.11.10",
    "ts-jest": "^29.3.1",
    "tslib": "2.8.1",
    "typescript": "5.8.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier --write"
    ]
  }
}
